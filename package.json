{
  "name": "content-moderation-api",
  "version": "1.0.0",
  "description": "Production-ready Fastify-based API service for content moderation using OpenAI API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts",
    "test": "node -r ./test-setup.js ./node_modules/.bin/tap",
    "test:watch": "node -r ./test-setup.js ./node_modules/.bin/tap --watch",
    "test:coverage": "node -r ./test-setup.js ./node_modules/.bin/tap --coverage",
    "test:jest": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "content-moderation",
    "api",
    "fastify",
    "openai",
    "twitter-api",
    "video-moderation",
    "image-moderation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^8.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fluent-ffmpeg": "^2.1.3",
    "oauth-1.0a": "^2.2.6",
    "openai": "^4.20.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.2.3",
    "twitter-api-v2": "^1.24.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tap": "^18.5.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
